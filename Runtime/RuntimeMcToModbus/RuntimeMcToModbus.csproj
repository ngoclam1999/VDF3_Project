<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AnalysisLevel>5.0-all</AnalysisLevel>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Ports" Version="9.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="EasyModbus">
      <HintPath>obj\EasyModbus.dll</HintPath>
    </Reference>
    <Reference Include="Interop.ACTETHERLib">
      <HintPath>obj\Interop.ACTETHERLib.dll</HintPath>
    </Reference>
    <Reference Include="Interop.ActUtlTypeLib">
      <HintPath>obj\Interop.ActUtlTypeLib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
